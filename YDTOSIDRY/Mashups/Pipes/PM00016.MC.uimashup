hor release="814" type="MashupModelAnchor" xrepPath="/SAP_BYD_UI_FLEX/STABLE_ANCHORS/MashupModelAnchor/00163eb1c2ba1edc9d8bba5b0deb2544.uianchor"><TargetFile xmlns="http://www.sap.com/a1s/cd/oberon/uimodelchange-1.0" xrepPath="/YDTOSIDRY_MAIN/SRC/Mashups/Pipes/PM00016.MC.uimashup"/></mashup:StableAnchor><mashup:Services id="00163eb1-b22b-1edc-9d8c-14d114ee9035"/><mashup:Pipes id="00163eb1-b22b-1edc-9d8c-14d114eeb035"><mashup:CustomPipe createdBy="K91JXP8GYQK" createdOn="2022-01-13T10:45:41Z" id="00163eb1-b22b-1edc-9d8c-14d114eed035" isActive="true" isManageable="true" isModifiable="true" isRemovable="true" lastChangedBy="K91JXP8GYQK" lastChangedOn="2022-01-13T11:01:26Z" mashupCategory="ZPopUp_MPB" mashupType="Html_Mashup" name="YDTOSIDRY_MAIN/PM00016" pipeType="CustomPipe" semanticCategory="" serviceProvider="" sourcePlug="Outport_In" targetPlug="Inport_Out"><mashup:DisplayName id="00163eb1-b22b-1edc-9d8c-14d114eef035" textPoolId="displayName"/><mashup:Description id="00163eb1-b22b-1edc-9d8c-14d114ef1035" textPoolId="description"/><mashup:Metadata id="00163eb1-b22b-1edc-9d8c-14d114ef3035" name="gadgetSource" value=""/><mashup:Metadata id="00163eb1-b22b-1edc-9d8c-14d114ef5035" name="widgetType" value="Advanced"/><mashup:Code><mashup:Source>&lt;![CDATA[&lt;!DOCTYPE html&gt;
&lt;html lang=&quot;en&quot;&gt;

&lt;head&gt;
    &lt;meta charset=&quot;UTF-8&quot;&gt;
    &lt;meta http-equiv=&quot;X-UA-Compatible&quot; content=&quot;IE=edge&quot;&gt;
    &lt;meta name=&quot;viewport&quot; content=&quot;width=device-width, initial-scale=1.0&quot;&gt;
    &lt;title&gt;Document&lt;/title&gt;
&lt;/head&gt;

&lt;body id=&quot;bd&quot;&gt;
    &lt;input type=&quot;file&quot; id=&quot;file1&quot; /&gt;

    &lt;script&gt;
        // get a reference to the file input
        const fileInput = document.getElementById(&quot;file1&quot;)

        // listen for the change event so we can capture the file
        fileInput.addEventListener(&quot;change&quot;, (e) =&gt; {
            // get a reference to the file
            const file = e.target.files[0];

            // encode the file using the FileReader API
            const reader = new FileReader();
            reader.onloadend = () =&gt; {
                // use a regex to remove data url part
                const base64String = reader.result
                    .replace(&quot;data:&quot;, &quot;&quot;)
                    .replace(/^.+,/, &quot;&quot;);

                //console.log(base64String);
                              var result_in = document.createElement(&quot;p&quot;);
                var data_in = document.createTextNode(sap.byd.ui.mashup.context.inport.ObjectID);
                result_in.appendChild(data_in);
                var body_data_in = document.getElementById(&quot;bd&quot;)
		body_data_in.appendChild( result_in)
              
                var result = document.createElement(&quot;p&quot;);
                var data = document.createTextNode(base64String);
                result.appendChild(data);
                var body_data = document.getElementById(&quot;bd&quot;)
                body_data.appendChild(result)
              
              sap.byd.ui.mashup.fireResultUpdate({&apos;BinaryData&apos;:base64String});

            };
            reader.readAsDataURL(file);
        });

    &lt;/script&gt;
&lt;/body&gt;

&lt;/html&gt;]]&gt;</mashup:Source></mashup:Code></mashup:CustomPipe></mashup:Pipes><mashup:Views id="00163eb1-b22b-1edc-9d8c-14d114ef7035" refreshOnInPortFired="true" refreshOnViewLoaded="true" style="inScreen"/></uxc:MashupModel><uxc:ModelChanges/><uxc:FieldTransformations/><uxc:ControllerImplementationPoints/><uxc:VirtualAssociations/></uxc:Implementation><Tags/><UXView id="00163eb1-b22b-1edc-9d8c-14d114ef9035" patternType="embedded"><uxv:ViewContainerRegion id="00163eb1-b22b-1edc-9d8c-14d114efb035"><uxv:View columns="1" id="00163eb1-b22b-1edc-9d8c-14d114efd035" rows="1" transparent="true"><uxv:PaneContainer id="00163eb1-b22b-1edc-9d8c-14d114eff035"><uxv:CustomPane assemblyName="../addins/sapLSUIMashupPanes.dll" bodyStyle="Transparent" fixedHeight="200" id="00163eb1-b22b-1edc-9d8c-14d114f01035" jsTypeName="sap.client.controls.core.MashupHTMLContainer" typeName="SAP.BYD.LS.UI.Mashup.Panes.CustomPanes.CustomJavascriptMashupPane"/></uxv:PaneContainer></uxv:View></uxv:ViewContainerRegion></UXView><TextPool><base:TextBlock currentLanguage="true" language="EN" masterLanguage="true"><base:TextPoolEntry id="00163eb1-b22b-1edc-9d8c-14d114f03035" text="Raise PopUp" textCategory="XTIT" textUuid="displayName"/><base:TextPoolEntry id="00163eb1-b22b-1edc-9d8c-14d114f05035" textCategory="YDES" textUuid="description"/></base:TextBlock></TextPool><Messages/><MessageMappings/></UXComponent>